# === Frontend .env.example ===

# Use mock API responses? (true/false)
VITE_USE_MOCK=false

# Base URL of the backend API server (no trailing slash)
VITE_API_URL=http://localhost:8000

# ChatGPT/OpenAI API key
VITE_OPENAI_API_KEY=

# WebSocket endpoint (for conversation realtime messaging)
VITE_WS_CHAT_URL=ws://localhost:8000/ws
